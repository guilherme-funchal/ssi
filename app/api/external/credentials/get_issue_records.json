{
  "meta": {
    "$_POST": [
      {
        "type": "text",
        "name": "credential_id"
      }
    ],
    "$_COOKIE": [
      {
        "type": "text",
        "name": "token"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "connection_id",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "be0ff970-b6b9-4942-85ac-4ab3ea40cd4f"
        }
      },
      {
        "name": "token",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{$_COOKIE.token}}",
          "key": "token"
        }
      },
      {
        "name": "api_credentials",
        "module": "api",
        "action": "send",
        "options": {
          "url": "https://{{$_ENV.REST_HOST}}:{{$_ENV.REST_PORT}}/issue-credential/records?connection_id={{connection_id}}",
          "schema": [],
          "headers": {
            "Authorization": "{{'Bearer '+token}}",
            "Accept": "application/json"
          },
          "dataType": "json",
          "passErrors": false
        },
        "output": true,
        "outputType": "object",
        "meta": [
          {
            "type": "object",
            "name": "data",
            "sub": [
              {
                "type": "array",
                "name": "results",
                "sub": [
                  {
                    "type": "text",
                    "name": "referent"
                  },
                  {
                    "type": "object",
                    "name": "attrs",
                    "sub": [
                      {
                        "type": "text",
                        "name": "cpf"
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "name": "schema_id"
                  },
                  {
                    "type": "text",
                    "name": "cred_def_id"
                  },
                  {
                    "type": "text",
                    "name": "rev_reg_id"
                  },
                  {
                    "type": "text",
                    "name": "cred_rev_id"
                  }
                ]
              }
            ]
          },
          {
            "type": "object",
            "name": "headers",
            "sub": [
              {
                "type": "text",
                "name": "connection"
              },
              {
                "type": "text",
                "name": "content-length"
              },
              {
                "type": "text",
                "name": "content-type"
              },
              {
                "type": "text",
                "name": "date"
              },
              {
                "type": "text",
                "name": "server"
              }
            ]
          }
        ]
      }
    ]
  }
}