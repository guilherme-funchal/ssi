{
  "meta": {
    "$_POST": [
      {
        "type": "text",
        "name": "credential_id"
      }
    ],
    "$_COOKIE": [
      {
        "type": "text",
        "name": "token"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "token",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{$_COOKIE.token}}",
          "key": "token"
        }
      },
      {
        "name": "api_credentials",
        "module": "api",
        "action": "send",
        "options": {
          "url": "https://{{$_ENV.REST_HOST}}:{{$_ENV.REST_PORT}}/credentials",
          "schema": [],
          "headers": {
            "Authorization": "{{'Bearer '+token}}",
            "Accept": "application/json"
          },
          "dataType": "json",
          "passErrors": false
        },
        "output": true,
        "outputType": "object",
        "meta": [
          {
            "type": "object",
            "name": "data",
            "sub": [
              {
                "type": "array",
                "name": "results",
                "sub": [
                  {
                    "type": "text",
                    "name": "referent"
                  },
                  {
                    "type": "object",
                    "name": "attrs",
                    "sub": [
                      {
                        "type": "text",
                        "name": "cpf"
                      }
                    ]
                  },
                  {
                    "type": "text",
                    "name": "schema_id"
                  },
                  {
                    "type": "text",
                    "name": "cred_def_id"
                  },
                  {
                    "type": "text",
                    "name": "rev_reg_id"
                  },
                  {
                    "type": "text",
                    "name": "cred_rev_id"
                  }
                ]
              }
            ]
          },
          {
            "type": "object",
            "name": "headers",
            "sub": [
              {
                "type": "text",
                "name": "connection"
              },
              {
                "type": "text",
                "name": "content-length"
              },
              {
                "type": "text",
                "name": "content-type"
              },
              {
                "type": "text",
                "name": "date"
              },
              {
                "type": "text",
                "name": "server"
              }
            ]
          }
        ]
      },
      {
        "name": "repeat",
        "module": "core",
        "action": "repeat",
        "options": {
          "repeat": "{{api_credentials.data.results}}",
          "outputFields": [],
          "exec": {
            "steps": {
              "name": "repeat1",
              "module": "core",
              "action": "repeat",
              "options": {
                "repeat": "{{attrs}}",
                "outputFields": [],
                "exec": {
                  "steps": [
                    {
                      "name": "atributo",
                      "module": "core",
                      "action": "setvalue",
                      "options": {
                        "value": "{{$name}}"
                      },
                      "output": true
                    },
                    {
                      "name": "valor",
                      "module": "core",
                      "action": "setvalue",
                      "options": {
                        "value": "{{$value}}"
                      },
                      "output": true
                    }
                  ]
                }
              },
              "output": true,
              "meta": [
                {
                  "name": "$index",
                  "type": "number"
                },
                {
                  "name": "$number",
                  "type": "number"
                },
                {
                  "name": "$name",
                  "type": "text"
                },
                {
                  "name": "$value",
                  "type": "object"
                },
                {
                  "name": "cpf",
                  "type": "text"
                },
                {
                  "name": "atributo",
                  "type": "text"
                },
                {
                  "name": "valor",
                  "type": "text"
                }
              ],
              "outputType": "array"
            }
          }
        },
        "output": true,
        "meta": [
          {
            "name": "$index",
            "type": "number"
          },
          {
            "name": "$number",
            "type": "number"
          },
          {
            "name": "$name",
            "type": "text"
          },
          {
            "name": "$value",
            "type": "object"
          },
          {
            "name": "referent",
            "type": "text"
          },
          {
            "name": "attrs",
            "type": "object",
            "sub": [
              {
                "name": "cpf",
                "type": "text"
              }
            ]
          },
          {
            "name": "schema_id",
            "type": "text"
          },
          {
            "name": "cred_def_id",
            "type": "text"
          },
          {
            "name": "rev_reg_id",
            "type": "text"
          },
          {
            "name": "cred_rev_id",
            "type": "text"
          },
          {
            "name": "repeat1",
            "type": "array",
            "sub": [
              {
                "name": "cpf",
                "type": "text"
              },
              {
                "name": "atributo",
                "type": "text"
              },
              {
                "name": "valor",
                "type": "text"
              }
            ]
          }
        ],
        "outputType": "array"
      }
    ]
  }
}