{
  "meta": {
    "$_GET": [
      {
        "type": "text",
        "name": "credential_id"
      }
    ],
    "$_COOKIE": [
      {
        "type": "text",
        "name": "token"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "token",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{$_COOKIE.token}}",
          "key": "token"
        }
      },
      {
        "name": "test_val",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "teste",
          "key": "test_val"
        }
      },
      {
        "name": "credential_id",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{$_GET.credential_id}}"
        }
      },
      {
        "name": "api_credentials",
        "module": "api",
        "action": "send",
        "options": {
          "url": "{{$_ENV.ACCESS+'://'+$_ENV.REST_HOST+':'+$_ENV.REST_PORT+'/credential/revoked/'+credential_id}}",
          "schema": [],
          "headers": {
            "Authorization": "{{'Bearer '+token}}",
            "Accept": "application/json"
          },
          "dataType": "json",
          "passErrors": false
        },
        "output": true,
        "outputType": "object",
        "meta": [
          {
            "type": "object",
            "name": "data",
            "sub": [
              {
                "type": "boolean",
                "name": "revoked"
              }
            ]
          },
          {
            "type": "object",
            "name": "headers",
            "sub": [
              {
                "type": "text",
                "name": "connection"
              },
              {
                "type": "text",
                "name": "content-length"
              },
              {
                "type": "text",
                "name": "content-type"
              },
              {
                "type": "text",
                "name": "date"
              },
              {
                "type": "text",
                "name": "server"
              }
            ]
          }
        ]
      }
    ]
  }
}