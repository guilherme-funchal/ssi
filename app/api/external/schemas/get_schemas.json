{
  "meta": {
    "$_GET": [
      {
        "type": "text",
        "name": "cred_id"
      }
    ],
    "$_POST": [
      {
        "type": "text",
        "name": "token"
      }
    ],
    "$_COOKIE": [
      {
        "type": "text",
        "name": "token"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "token",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{$_COOKIE.token}}",
          "key": "token"
        }
      },
      {
        "name": "api_schemas",
        "module": "api",
        "action": "send",
        "options": {
          "url": "http://161.148.151.16:8021/schemas/created",
          "schema": [],
          "headers": {
            "Authorization": "{{'Bearer '+token}}",
            "Accept": "application/json"
          },
          "dataType": "json",
          "passErrors": false
        },
        "output": true,
        "outputType": "object",
        "meta": [
          {
            "type": "object",
            "name": "data",
            "sub": [
              {
                "type": "array",
                "name": "schema_ids",
                "sub": [
                  {
                    "type": "text",
                    "name": "$value"
                  }
                ]
              }
            ]
          },
          {
            "type": "object",
            "name": "headers",
            "sub": [
              {
                "type": "text",
                "name": "content-length"
              },
              {
                "type": "text",
                "name": "content-type"
              },
              {
                "type": "text",
                "name": "date"
              },
              {
                "type": "text",
                "name": "server"
              }
            ]
          }
        ]
      },
      {
        "name": "repeat",
        "module": "core",
        "action": "repeat",
        "options": {
          "repeat": "{{api_schemas.data}}",
          "outputFields": [],
          "exec": {
            "steps": {
              "name": "id",
              "module": "core",
              "action": "setvalue",
              "options": {
                "value": "{{$value}}"
              },
              "output": true
            }
          }
        },
        "output": true,
        "meta": [
          {
            "name": "$index",
            "type": "number"
          },
          {
            "name": "$number",
            "type": "number"
          },
          {
            "name": "$name",
            "type": "text"
          },
          {
            "name": "$value",
            "type": "object"
          },
          {
            "name": "schema_ids",
            "type": "array",
            "sub": [
              {
                "name": "$value",
                "type": "text"
              }
            ]
          },
          {
            "name": "id",
            "type": "text"
          }
        ],
        "outputType": "array"
      }
    ]
  }
}