{
  "meta": {
    "$_GET": [
      {
        "type": "text",
        "name": "schema_id"
      }
    ],
    "$_POST": [
      {
        "type": "text",
        "name": "credential_id"
      }
    ],
    "$_COOKIE": [
      {
        "type": "text",
        "name": "token"
      }
    ]
  },
  "exec": {
    "steps": [
      {
        "name": "schema_id",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{$_GET.schema_id}}"
        }
      },
      {
        "name": "token",
        "module": "core",
        "action": "setvalue",
        "options": {
          "value": "{{$_COOKIE.token}}",
          "key": "token"
        }
      },
      {
        "name": "api_credentials",
        "module": "api",
        "action": "send",
        "options": {
          "url": "https://{{$_ENV.REST_HOST}}:{{$_ENV.REST_PORT}}/schemas/{{schema_id}}",
          "schema": [],
          "headers": {
            "Authorization": "{{'Bearer '+token}}",
            "Accept": "application/json"
          },
          "dataType": "json",
          "passErrors": false
        },
        "output": true,
        "outputType": "object",
        "meta": [
          {
            "type": "object",
            "name": "data",
            "sub": [
              {
                "type": "object",
                "name": "schema",
                "sub": [
                  {
                    "type": "text",
                    "name": "ver"
                  },
                  {
                    "type": "text",
                    "name": "id"
                  },
                  {
                    "type": "text",
                    "name": "name"
                  },
                  {
                    "type": "text",
                    "name": "version"
                  },
                  {
                    "type": "array",
                    "name": "attrNames",
                    "sub": [
                      {
                        "type": "text",
                        "name": "$value"
                      }
                    ]
                  },
                  {
                    "type": "number",
                    "name": "seqNo"
                  }
                ]
              }
            ]
          },
          {
            "type": "object",
            "name": "headers",
            "sub": [
              {
                "type": "text",
                "name": "content-length"
              },
              {
                "type": "text",
                "name": "content-type"
              },
              {
                "type": "text",
                "name": "date"
              },
              {
                "type": "text",
                "name": "server"
              }
            ]
          }
        ]
      },
      {
        "name": "api_creddef",
        "module": "api",
        "action": "send",
        "options": {
          "url": "https://{{$_ENV.REST_HOST}}:{{$_ENV.REST_PORT}}/credential-definitions/created",
          "schema": [],
          "headers": {
            "Authorization": "{{'Bearer '+token}}",
            "Accept": "application/json"
          },
          "dataType": "json",
          "passErrors": false,
          "params": {
            "schema_id": "{{schema_id}}"
          }
        },
        "output": true,
        "outputType": "object",
        "meta": [
          {
            "type": "object",
            "name": "data",
            "sub": [
              {
                "type": "array",
                "name": "credential_definition_ids",
                "sub": [
                  {
                    "type": "text",
                    "name": "$value"
                  }
                ]
              }
            ]
          },
          {
            "type": "object",
            "name": "headers",
            "sub": [
              {
                "type": "text",
                "name": "content-length"
              },
              {
                "type": "text",
                "name": "content-type"
              },
              {
                "type": "text",
                "name": "date"
              },
              {
                "type": "text",
                "name": "server"
              }
            ]
          }
        ]
      },
      {
        "name": "repeat",
        "module": "core",
        "action": "repeat",
        "options": {
          "repeat": "{{api_credentials.data.schema.attrNames}}",
          "outputFields": [],
          "exec": {
            "steps": {
              "name": "atributo",
              "module": "core",
              "action": "setvalue",
              "options": {
                "value": "{{$value}}"
              },
              "output": true
            }
          }
        },
        "output": true,
        "meta": [
          {
            "name": "$index",
            "type": "number"
          },
          {
            "name": "$number",
            "type": "number"
          },
          {
            "name": "$name",
            "type": "text"
          },
          {
            "name": "$value",
            "type": "text"
          },
          {
            "name": "atributo",
            "type": "text"
          }
        ],
        "outputType": "array"
      }
    ]
  }
}