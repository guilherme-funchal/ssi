{
  "name": "api",
  "module": "api",
  "action": "send",
  "options": {
    "url": "https://{{_ENV_REST_HOST}}:{{_ENV_REST_PORT}}/status/live",
    "schema": [],
    "dataType": "json"
  },
  "output": true,
  "meta": [
    {
      "type": "object",
      "name": "data",
      "sub": [
        {
          "type": "boolean",
          "name": "alive"
        }
      ]
    },
    {
      "type": "object",
      "name": "headers",
      "sub": [
        {
          "type": "text",
          "name": "connection"
        },
        {
          "type": "text",
          "name": "content-length"
        },
        {
          "type": "text",
          "name": "content-type"
        },
        {
          "type": "text",
          "name": "date"
        },
        {
          "type": "text",
          "name": "server"
        }
      ]
    }
  ],
  "outputType": "object"
}